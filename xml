      <title>  $('td.none a.subscribe').on('click', function(e) {
    var item_type = $(this).attr('data-type');
    var item_id = $(this).attr('data-id');

    $.ajax({
      url: '/notifications/subscribe_to_group',
      type: 'PUT',
      data: {
        media_type: item_type,
        media_id: item_id
      }
    }).fail(function() {
      showError('A intervenit o problemă.');
    }).done(function(response) {

      if (response.failure) {
        showError('A intervenit o problemă.');
      }

      if (response.success) {
        showSuccess('Subscribed', 'Succes');
        $('td.none p.notify').toggleClass('hide');
      }

    });
  });

  $('td.none a.unsubscribe').on('click', function(e) {
    var item_type = $(this).attr('data-type');
    var item_id = $(this).attr('data-id');

    $.ajax({
      url: '/notifications/subscribe_to_group',
      type: 'DELETE',
      data: {
        media_type: item_type,
        media_id: item_id
      }
    }).fail(function() {
      showError('A intervenit o problemă.');
    }).done(function(response) {

      if (response.failure) {
        showError('A intervenit o problemă.');
      }

      if (response.success) {
        showSuccess('Unsubscribed', 'Succes');
        $('td.none p.notify').toggleClass('hide');
      }

    });
  });Media</title>
