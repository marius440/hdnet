          <para>   $(function() {

    $("#tv_report_window_report_form input.report.submit").on('click', function(e){
      e.preventDefault();
      var selected_submit_button = $(this);
      selected_submit_button.prop('disabled', true);

      $.ajax({
        url: '/modal/report/new',
        type: 'POST',
        data: serializeAndEscapeForm($('#tv_report_window_report_form')),
      }).fail(function() {
        showError('A intervenit o problemă.');
        selected_submit_button.prop('disabled', false);
      }).done(function(response) {
        selected_submit_button.prop('disabled', false);

        if (response.failure &amp;&amp; response.failure.errors) {
          showError(response.failure.errors, response.failure.title);
        }

        if (response.success) {
          showSuccess('Raportul dumneavoastră a fost trimis cu succes.');
          $("#tv_report_window").data("kendoWindow").close();
        }
      });
    });

