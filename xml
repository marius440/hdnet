      $("#rating_input").on("rating:change", function(event, value, caption) {
        $.ajax({
          url: '/tv/84958-loki/remote/rating?translate=false',
          type: 'POST',
          data: {
            rating: (value * 2)
          }
        }).fail(function() {
          showError('There was a problem rating this item.');
        }).done(function(response) {
          if (response.failure) {
            showError('There was a problem rating this item.');
          }

          if (response.success) {
            var rating_pressed = false;
            showSuccess('Nota dumneavoastră a fost salvată.', 'Succes');

            if (!$('#rate_it span').hasClass('true')) {
              $('#rate_it span').addClass('true');
            }

            
            if ($('#watchlist span').hasClass('true')) {
              $('#watchlist span').removeClass('true');
              $('#watchlist span').find('span.text').html('Adaugă în lista filmelor de urmărit');
            }
            

            $('ul.actions li.rating.tooltip_hover').attr('title', response.text);
          }
        });
      });

      $('#rating_input').on('rating:clear', function(event) {
        $.ajax({
          url: '/u/jucaria123/rated?media_type=tv&amp;media_id=84958',
          dataType: "json",
          type: 'DELETE'
        }).fail(function() {
            showError('There was a problem removing this rating.');
        }).done(function(response) {
          if (response.failure) {
            showError('There was a problem removing this rating.');
          }

          if (response.success) {
            if ($('#rate_it span').hasClass('true')) {
              $('#rate_it span').removeClass('true');
            }

            $('ul.actions li.rating.tooltip_hover').attr('title', response.text);
          }
        });
      });

      $("#rating_input").rating({
        animate: false,
